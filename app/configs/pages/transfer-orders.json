{
    "title": "Перенос заявки",
    "header": "Перенос заявки",
    "bodyclass": "fixed-header",
    "layout": "default.datatables",
    "includes":["dt-crud-modals"],
    "datatables":{
        "default":{
            "tableId":"default",
            "buttons" : {"create":true,"edit":true,"delete":true},
            "crud_modals":{
                "titles":{
                    "create":"Добавить",
                    "edit":"Редактировать",
                    "delete":"Удалить"
                },
                "fields": [

                    {
                        "label": "Код заявки",
                        "type":"text",
                        "property": "Код_Заявки"
                    },
                    {
                        "label": "Перенос срока с",
                        "type":"date",
                        "property": "Дата_переноса"
                    },
                    {
                        "label": "Дата служебной записки",
                        "type":"date",
                        "property": "Дата_Служебной"
                    },
                    {
                        "label": "Причина переноса",
                        "type":"text",
                        "property": "Причина_Переноса"
                    },
                    {
                        "label": "Примечание",
                        "type": "text",
                        "property": "Примечание"
                    }
                ],

                "subheaders":{"delete":"Вы уверены что хотите удалить \"<span data-obj=filename></span>\"?"}
            }
        }
    }
}
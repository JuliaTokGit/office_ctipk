{
    "title": "Entries",
    "header": "Entries",
    "bodyclass": "fixed-header",
    "layout": "default.datatables",
    "includes": ["dt-crud-modals","filters"],
    "quickview":true,
    "filtering": {"gl_account_id":true,"currency_id":true},
    "datatables":{
        "default":{
            "tableId":"default",
            "buttons" : {},
            "crud_modals":{
                "titles":{
                    "create":"Add entry",
                    "edit":"Edit entry",
                    "delete":"Delete entry"
                },
                "fields": [

                            {
                                "label":"Name",
                                "type":"text",
                                "property": "name"
                            },
                            {
                                "label":"Symbol",
                                "type":"text",
                                "property": "symbol"
                            }
                        ],

                "subheaders":{"delete":"Please confirm deletion \"<span data-obj=name></span>\" ?"}
            }
        }
    }
}

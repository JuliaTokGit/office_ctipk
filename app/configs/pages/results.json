{
    "title": "Результаты",
    "header": "Результаты",
    "bodyclass": "fixed-header",
    "layout": "default.datatables",
    "includes":["dt-crud-modals","filters"],
    "quickview":true,
    "filtering": {"user_id":true},
    "datatables":{
        "default":{
            "tableId":"default",
            "buttons" : {"create":true,"delete":true},
            "crud_modals":{
                "titles":{
                    "create":"Добавить результат",
                    "edit":"Редактировать результат",
                    "delete":"Удалить результат"
                },
                "fields": [{
                    "label": "Запрос",
                    "type":"select_ajax",
                    "property": "request_id",
                    "url":"/select-ajax/requests"
                },
                            {   "label":"Файл",
                                "type":"file",
                                "property":"upload_id",
                                "btn-text":"Выберите файл для загрузки",
                                "accept":"text/csv"
                            }
                        ],

                "subheaders":{"delete":"Вы уверены что хотите удалить \"<span data-obj=filename></span>\"?"}
            }
        }
    }
}

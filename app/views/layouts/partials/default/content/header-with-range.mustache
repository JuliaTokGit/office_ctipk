<div class="container-fluid container-fixed-lg m-b-10 pageheader">

                
                <!-- START BREADCRUMB -->
                <ul class="breadcrumb">
                  {{#breadcrumbs}}
                  <li>
                    <a class="active" href="{{link}}">{{title}}</a>
                  </li>
                  {{/breadcrumbs}}
                </ul>
                <!-- END BREADCRUMB -->
                
<div class="row m-0">
  <div class="col-xs-6 col-sm-8 col-md-9 col-xlg-10">
                  {{#page.sup_header}}<h5>{{.}}</h5>{{/page.sup_header}}
              {{^breadcrumbs}}{{^page.sup_header}}<h5>&nbsp;</h5>{{/page.sup_header}}{{/breadcrumbs}}
              {{#page.header}}<h3 class="page-title">{{.}} {{#page.header_label}}<span class="label {{class}}" style="vertical-align: 40%;">{{name}}</span>{{/page.header_label}}</h3>{{/page.header}}
              {{#page.sub_header}}<h5>{{.}}</h5>{{/page.sub_header}}
  </div>
  <div class="col-xs-6 col-sm-4 col-md-3 col-xlg-2 text-right">
    <form role="form" style="margin:-3px 0 -3px; white-space:nowrap;" method="POST">
      <div class="input-group">
        <input type="hidden" name="action" value="set_range">
        <input id="range-calendar" type="text" name="range" value="{{page.jsvars.range}}" class="dpicker form-control"/>
        <span class="input-group-addon"><i class="fa fa-calendar"></i></span>

        {{! <button type="button" class="btn btn-default btn-xs open-range-calendar"><i class="fs-14 pg-calender"></i></button> }}
      </div>
    </form>    
  </div>
</div>



</div>

<!-- START HEADER -->
<div class="header ">
<div class="container-fluid container-fixed-lg">
	{{> layout.header.mobile-controls}}
	<div class=" pull-left sm-table hidden-xs hidden-sm">
		<div class="header-inner">
			<div class="brand inline">
				{{#site.logo}}
				<img {{{ site.logo | src }}}  alt="{{ site.logo.alt }}" width="{{ site.logo.width }}" height="{{ site.logo.height }}">
				{{/site.logo}}
				{{^site.logo}}
				<h2 class="m-t-5 text-nowrap">{{site.name}}</h2>
				{{/site.logo}}
			</div>

			{{# layout.header-buttons-and-notification }}
			{{> layout.header.notification-list}}
			{{/ layout.header-buttons-and-notification }}

			{{# layout.search }}
			<a href="#" class="search-link" data-toggle="search"><i class="pg-search"></i>Type anywhere to <span class="bold">search</span></a>
			{{/ layout.search }}
		</div>
	</div>



	<div class=" pull-right">
		{{> layout.header.user-info}}
	</div>
	</div>
</div>
<!-- END HEADER -->
